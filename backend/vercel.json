{
    "version": 2,
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/api-docs(.*)",
            "dest": "dist/main.js",
            "methods": ["GET"]
        },
        {
            "src": "/api/game/(.*)",
            "dest": "dist/main.js",
            "methods": ["POST", "OPTIONS"]
        },
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": ["GET", "POST", "OPTIONS"]
        }
    ]
}
